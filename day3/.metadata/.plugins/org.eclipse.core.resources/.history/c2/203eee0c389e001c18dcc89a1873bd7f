package com.exceptionHandling;

public class PriorityClass implements Runnable {
	private String name ;
	
	public PriorityClass(String name) {
		this.name = name ;
	}
	
	public void display() {
		System.out.println("["+name);
		System.out.println("]");
	}
	
	
	@Override
	public void run() {
		display();
	}

}
