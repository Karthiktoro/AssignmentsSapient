		l.add("karthik");
		l.add("rahul");
		l.add("thash");
		l.add("gowda");
		l.add("mad");
		l.add("game");
		l.add("madhan");
		l.add("lekhan");
		l.add("modi");
		System.out.println(l);
		System.out.println(l.contains("rahul"));
		
		Iterator<String> itr= new Iterator<String>() {
			
			@Override
