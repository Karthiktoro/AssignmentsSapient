package com.exceptionHandling;

public class MainDemo1 {

	public static void main(String[] args) {
		MyDemo1 d1 = new MyDemo1();
		MyDemo1 d2 = new MyDemo1();
		MyDemo1 d3 = new MyDemo1();
		d1.start();
		d2.start();
		d3.start();
		try {
			d1.sleep(1000);
			d2.sleep(1000);
			d3.sleep(1000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		

		

	}

}
